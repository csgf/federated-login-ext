<?xml version="1.0"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
	<action-mappings>
                <action path="/login/stork" type="com.liferay.portlet.login.action.STORKAction">
                        <forward name="portlet.login.login" path="portlet.login.login" />
                        <forward name="portlet.login.stork.country" path="portlet.login.stork.country" />
                        <forward name="portlet.login.stork.peps" path="portlet.login.stork.peps" />
                        <forward name="portlet.login.stork.error" path="portlet.login.stork.error" />
                        <forward name="portlet.login.stork.notAuth" path="portlet.login.stork.notAuth" />
                </action>
	</action-mappings>
</struts-config>
